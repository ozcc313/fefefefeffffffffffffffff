        callback = function(value)
        getgenv().Settings.Silent.Enabled = (Value)
    if getgenv().Settings.Silent.Enabled == true then

            print("Silent Aim is Enabled")
    end
    if getgenv().Settings.Silent.Enabled == false then
            print("Silent Aim is Enabled")
            end
end
})
